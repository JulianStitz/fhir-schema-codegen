{
  "name": "@fhirschema/codegen",
  "version": "0.0.4",
  "author": "niquola",
  "license": "ISC",
  "main": "dist/cli.js",
  "bin": {
    "fscg": "./dist/cli.js",
    "fhirschema-codegen": "./dist/cli.js"
  },
  "source": "src/cli.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fhir-schema/fhir-schema-codegen.git"
  },
  "bugs": {
    "url": "https://github.com/fhir-schema/fhir-schema-codegen/issues"
  },
  "scripts": {
    "build": "tsc && npm run copy-static",
    "copy-static": "copyfiles -u 2 \"src/generators/*/static/**/*\" dist/generators",
    "start": "node dist/cli.js",
    "start:dev": "tsx watch src/cli.ts generate --generator csharp --output /Users/gena.razmakhnin/Documents/sdk-gen/MyConsoleApp/aidbox --files /Users/gena.razmakhnin/Documents/typeschema/output/hl7.fhir.r4.core@4.0.1.ndjson",
    "start:dev:ts": "tsx watch src/cli.ts generate --generator typescript --output /Users/gena.razmakhnin/Documents/sdk-gen/ts-example/aidbox --files /Users/gena.razmakhnin/Documents/typeschema/output/hl7.fhir.r4.core@4.0.1.ndjson",
    "start:dev:ts:local": "tsx watch src/cli.ts generate --generator typescript --output ./output --files ../type-schema/output/hl7.fhir.r4.core@4.0.1.ndjson",
    "start:dev:python": "tsx watch src/cli.ts generate --generator python --output /Users/gena.razmakhnin/Documents/sdk-gen/python-example/aidbox --files /Users/gena.razmakhnin/Documents/typeschema/output/hl7.fhir.r4.core@4.0.1.ndjson",
    "dev": "tsc -w",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint src",
    "format": "biome format src",
    "check": "biome check src",
    "check:fix": "biome check --fix src",
    "get-schemas": "mkdir -p data && curl -L 'https://storage.googleapis.com/fhir-schema-registry/1.0.0/hl7.fhir.r4.core%234.0.1/package.ndjson.gz' | gunzip > data/hl7.fhir.r4.core.ndjson"
  },
  "keywords": [
    "fhir",
    "fhirschema",
    "codegen",
    "typescript",
    "javascript"
  ],
  "description": "",
  "devDependencies": {
    "@type-challenges/utils": "^0.1.1",
    "@types/node": "^22.8.7",
    "@types/node-fetch": "^2.6.12",
    "@vitest/coverage-v8": "^3.0.9",
    "copyfiles": "^2.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@biomejs/biome": "^2.0.0-beta.1",
    "commander": "13.1.0",
    "picocolors": "^1.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
